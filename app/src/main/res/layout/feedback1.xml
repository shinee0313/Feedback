<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="You have arrived!"
        android:textColor="#2B0606"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="128dp"
        tools:layout_editor_absoluteY="56dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Please rate my service."
        android:textColor="#000000"
        android:textColorHint="#0C0A0A"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="102dp"
        tools:layout_editor_absoluteY="83dp" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Late"
        android:textColor="#000000"
        tools:layout_editor_absoluteX="69dp"
        tools:layout_editor_absoluteY="311dp" />

    <CheckBox
        android:id="@+id/checkBox4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dangerous driving"
        tools:layout_editor_absoluteX="69dp"
        tools:layout_editor_absoluteY="448dp" />

    <CheckBox
        android:id="@+id/checkBox2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fare too high"
        android:textColor="#000000"
        tools:layout_editor_absoluteX="69dp"
        tools:layout_editor_absoluteY="359dp" />

    <CheckBox
        android:id="@+id/checkBox3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Unfriendly"
        tools:layout_editor_absoluteX="69dp"
        tools:layout_editor_absoluteY="400dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Any comments?"
        tools:layout_editor_absoluteX="155dp"
        tools:layout_editor_absoluteY="513dp" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="286dp"
        android:layout_height="42dp"
        android:ems="10"
        android:hint="Your comment"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="62dp"
        tools:layout_editor_absoluteY="532dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rate"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="589dp" />

</androidx.constraintlayout.widget.ConstraintLayout>